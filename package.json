{
  "name": "robot-viewer",
  "version": "1.2.1",
  "description": "Web-based robot model viewer supporting URDF, MJCF, and USD formats",
  "main": "index.html",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "robot",
    "urdf",
    "xacro",
    "mjcf",
    "usd",
    "three.js",
    "viewer"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "^7.23.0",
    "@codemirror/autocomplete": "^6.12.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-xml": "^6.0.2",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.0",
    "@uiw/codemirror-theme-dracula": "^4.21.21",
    "@uiw/codemirror-theme-github": "^4.21.21",
    "@uiw/codemirror-theme-vscode": "^4.21.21",
    "codemirror": "^6.0.1",
    "d3": "^7.9.0",
    "mujoco-js": "^0.0.7",
    "three": "^0.163.0",
    "urdf-loader": "^0.12.5",
    "xacro-parser": "^0.3.11"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^25.2.3",
    "@types/three": "^0.182.0",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "terser": "^5.26.0",
    "typescript": "^5.9.3",
    "vite": "^4.5.0",
    "vitest": "^4.0.18"
  }
}
